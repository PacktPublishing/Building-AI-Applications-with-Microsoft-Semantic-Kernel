{
    "schema": 1,
    "type": "completion",
    "description": "Solves a word problem",
    "default_services": [
        "gpt35"
    ],
    "execution_settings": {
        "default": {
            "temperature": 0.8,
            "number_of_responses": 1,
            "top_p": 1,
            "max_tokens": 4000,
            "presence_penalty": 0.0,
            "frequency_penalty": 0.0
        }
    },
    "input_variables": [
        {
            "name": "problem",
            "description": "The problem that needs to be solved",
            "required": true
        }
    ]
}